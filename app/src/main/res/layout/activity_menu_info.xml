<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="aar.businessinspain.MenuSpain"
    >


    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:orientation="horizontal"
        android:background="#fff3c428"
        android:weightSum="1"
        android:padding="20dp"
        android:layout_gravity="center_horizontal">

        <TextView
            style="@style/titulos"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:text="Antes de Partir"
            android:id="@+id/textView"
            android:gravity="center_vertical" />

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:background="#ffffffff"
        android:padding="5dp">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="#ffffffff"
            android:layout_weight="1">

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:id="@+id/imageViewInternet"
                android:src="@drawable/internet"
                android:layout_weight="0.12"
                android:layout_gravity="center_horizontal"
                android:onClick="advanceToInternet"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Telefonía e Internet"
                android:id="@+id/textView10"
                android:layout_weight="0.12"
                android:layout_gravity="center_horizontal" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="#ffffffff"
            android:layout_weight="1" >

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:id="@+id/imageViewInteres"
                android:src="@drawable/interes"
                android:layout_weight="0.12"
                android:layout_gravity="center_horizontal"
                android:onClick="advanceToInteres"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Lugares de Interés"
                android:id="@+id/textView11"
                android:layout_weight="0.12"
                android:layout_gravity="center_horizontal" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:background="#ffffffff"
        android:layout_gravity="center_horizontal"
        android:padding="5dp" >

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="#ffffffff"
            android:layout_weight="1" >

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:id="@+id/imageViewAerolineas"
                android:src="@drawable/aerolineas"
                android:layout_weight="0.12"
                android:layout_gravity="center_horizontal"
                android:onClick="advanceToAerolineas"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Aerolíneas"
                android:id="@+id/textView12"
                android:layout_weight="0.12"
                android:layout_gravity="center_horizontal"
                android:gravity="center" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="#ffffffff"
            android:layout_weight="1" >

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:id="@+id/imageViewSalud"
                android:src="@drawable/salud"
                android:layout_weight="0.12"
                android:layout_gravity="center_horizontal"
                android:onClick="advanceToSalud"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Salud"
                android:id="@+id/textView13"
                android:layout_weight="0.12"
                android:layout_gravity="center_horizontal" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="#ffffffff"
            android:layout_weight="1" >

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:id="@+id/imageViewClima"
                android:src="@drawable/clima"
                android:layout_weight="0.12"
                android:layout_gravity="center_horizontal"
                android:onClick="advanceToClima"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Clima"
                android:id="@+id/textView14"
                android:layout_weight="0.12"
                android:layout_gravity="center_horizontal" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:background="#ffffffff"
        android:padding="5dp" >

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="#ffffffff"
            android:layout_weight="1" >

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:id="@+id/imageViewVisa"
                android:src="@drawable/visa"
                android:layout_weight="0.12"
                android:layout_gravity="center_horizontal"
                android:onClick="advanceToVisa"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="VISA y Pasaporte"
                android:id="@+id/textView15"
                android:layout_weight="0.12"
                android:layout_gravity="center_horizontal" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:background="#ffffffff"
            android:layout_weight="1" >

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:id="@+id/imageViewAranceles"
                android:src="@drawable/aranceles"
                android:layout_weight="0.12"
                android:layout_gravity="center_horizontal"
                android:onClick="advanceToAranceles"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Trámites Arancelarios"
                android:id="@+id/textView16"
                android:layout_weight="0.12"
                android:layout_gravity="center_horizontal" />
        </LinearLayout>
    </LinearLayout>


</LinearLayout>