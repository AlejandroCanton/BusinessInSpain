<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="aar.businessinspain.MenuSpain"
    >


    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:orientation="horizontal"
        android:background="#fff3c428"
        android:weightSum="1"
        android:padding="20dp"
        android:layout_gravity="center_horizontal">

        <TextView
            style="@style/titulos"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:text="Protocolo"
            android:id="@+id/textView"
            android:gravity="center_vertical" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:background="#fff4fff5">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#ffffffff"
            android:gravity="center_vertical"
            android:weightSum="1">


            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:id="@+id/imageViewModelo"
                android:src="@drawable/modelo"
                android:layout_weight="0.06"
                android:layout_gravity="top|center_horizontal"
                android:onClick="advanceToModelo"
                />

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="Modelo de ComunicaciÃ³n"
                android:id="@+id/textView10"
                android:layout_weight="0.06"
                android:layout_gravity="center_horizontal"
                android:gravity="center"
                android:layout_marginBottom="35dp" />

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:id="@+id/imageViewVestimenta"
                android:src="@drawable/vestimenta"
                android:layout_weight="0.06"
                android:layout_gravity="center_horizontal"
                android:onClick="advanceToVestimenta"/>

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="Codigo de Vestimenta"
                android:id="@+id/textView11"
                android:layout_weight="0.06"
                android:gravity="center"
                android:layout_gravity="center_horizontal"
                android:layout_marginBottom="35dp" />


            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:id="@+id/imageViewAgenda"
                android:src="@drawable/agenda"
                android:layout_weight="0.06"
                android:layout_gravity="center_horizontal"
                android:onClick="advanceToAgenda"/>

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="Agenda"
                android:id="@+id/textView12"
                android:layout_weight="0.06"
                android:gravity="center"
                android:layout_gravity="center_horizontal" />


        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#ffffffff"
            android:gravity="center_vertical"
            android:weightSum="1">


            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:id="@+id/imageViewAspectos"
                android:src="@drawable/generales"
                android:layout_weight="0.06"
                android:layout_gravity="top|center_horizontal"
                android:onClick="advanceToAspectos"/>

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="Aspectos Generales"
                android:id="@+id/textView13"
                android:layout_weight="0.06"
                android:layout_gravity="center_horizontal"
                android:gravity="center"
                android:layout_marginBottom="35dp" />

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:id="@+id/imageViewAlimentos"
                android:src="@drawable/alimentos"
                android:layout_weight="0.06"
                android:layout_gravity="center_horizontal"
                android:onClick="advanceToAlimentos"/>

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="Horario de Alimentos"
                android:id="@+id/textView14"
                android:layout_weight="0.06"
                android:gravity="center"
                android:layout_gravity="center_horizontal"
                android:layout_marginBottom="35dp" />

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:id="@+id/imageViewRecomendaciones"
                android:src="@drawable/recomendaciones"
                android:layout_weight="0.06"
                android:layout_gravity="center_horizontal"
                android:onClick="advanceToRecomendaciones"/>

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="Recomendaciones"
                android:id="@+id/textView15"
                android:layout_weight="0.06"
                android:gravity="center"
                android:layout_gravity="center_horizontal" />


        </LinearLayout>


</LinearLayout>


</LinearLayout>