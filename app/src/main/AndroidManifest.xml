<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="aar.businessinspain" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:theme="@style/AppTheme" >
        <activity android:name="aar.businessinspain.Main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="aar.businessinspain.MenuInicio"
            android:label="@string/title_activity_menu_inicio"
            android:parentActivityName="aar.businessinspain.Main" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="aar.businessinspain.Main" />
        </activity>
    </application>

</manifest>
